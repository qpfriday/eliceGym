{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, reactive } from \"vue\";\nimport axios from \"axios\";\nimport { addCommas } from \"@/scripts/lib\";\nimport router from \"@/scripts/router\";\nexport default {\n  setup() {\n    const state = reactive({\n      items: [],\n      form: {\n        name: \"\",\n        address: \"\",\n        payment: \"\",\n        cardNumber: \"\",\n        items: \"\"\n      }\n    });\n    const load = () => {\n      axios.get(\"/api/cart/items\").then(({\n        data\n      }) => {\n        console.log(data);\n        state.items = data;\n      });\n    };\n    const submit = () => {\n      const args = JSON.parse(JSON.stringify(state.form));\n      args.items = JSON.stringify(state.items);\n      axios.post(\"api/orders\", args).then(() => {\n        alert('주문을 완료하였습니다.');\n        router.push({\n          path: \"/orders\"\n        });\n      });\n    };\n    const computedPrice = computed(() => {\n      let result = 0;\n      for (let i of state.items) {\n        result += i.price - i.price * i.discountPer / 100;\n      }\n      return result;\n    });\n    load();\n    return {\n      state,\n      addCommas,\n      computedPrice,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["computed","reactive","axios","addCommas","router","setup","state","items","form","name","address","payment","cardNumber","load","get","then","data","console","log","submit","args","JSON","parse","stringify","post","alert","push","path","computedPrice","result","i","price","discountPer"],"sources":["C:\\Users\\modev\\Desktop\\chan\\gallery\\frontend\\src\\pages\\Order.vue"],"sourcesContent":["<script>\r\nimport {computed, reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport {addCommas} from \"@/scripts/lib\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      form: {\r\n        name:\"\",\r\n        address:\"\",\r\n        payment:\"\",\r\n        cardNumber:\"\",\r\n        items:\"\",\r\n\r\n      }\r\n    })\r\n    const load = () => {\r\n      axios.get(\"/api/cart/items\").then(({data}) => {\r\n        console.log(data);\r\n        state.items = data;\r\n      })\r\n    }\r\n    const submit = () => {\r\n      const args = JSON.parse(JSON.stringify(state.form));\r\n      args.items = JSON.stringify(state.items);\r\n      axios.post(\"api/orders\", args).then(() => {\r\n        alert('주문을 완료하였습니다.');\r\n        router.push({path: \"/orders\"})\r\n      });\r\n    };\r\n\r\n    const computedPrice = computed(() => {\r\n      let result =0;\r\n      for (let i of state.items) {\r\n        result += i.price - i.price * i.discountPer / 100\r\n      }\r\n      return result;\r\n    });\r\n\r\n    load();\r\n\r\n    return {state, addCommas, computedPrice, submit}\r\n  },\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"order\">\r\n    <div class=\"container\">\r\n      <main>\r\n        <div class=\"py-5 text-center\"><h2>주문하기</h2>\r\n          <p class=\"lead\">Below is an example form built entirely with Bootstrap’s form controls. Each required form\r\n            group has a validation state that can be triggered by attempting to submit the form without completing\r\n            it.</p></div>\r\n        <div class=\"row g-5\">\r\n          <div class=\"col-md-5 col-lg-4 order-md-last\">\r\n            <h4 class=\"d-flex justify-content-between align-items-center mb-3\">\r\n              <span class=\"text-primary\">장바구니 목록</span>\r\n              <span class=\"badge bg-primary rounded-pill\">{{ state.items.length }}</span>\r\n            </h4>\r\n            <ul class=\"list-group mb-3\">\r\n              <li class=\"list-group-item d-flex justify-content-between lh-sm\" v-for=\"(i, idx) in state.items\" :key=\"idx\">\r\n                <div><h6 class=\"my-0\">{{ i.name }}</h6></div>\r\n                <span class=\"text-body-secondary\">\r\n                  {{ addCommas(i.price - i.price * i.discountPer / 100) }} 원\r\n                </span></li>\r\n            </ul>\r\n            <h2 class=\"text-center total-price\">\r\n              {{ addCommas(computedPrice) }} 원\r\n            </h2>\r\n          </div>\r\n          <div class=\"col-md-7 col-lg-8\"><h4 class=\"mb-3\">주문 정보</h4>\r\n            <div class=\"needs-validation\" novalidate=\"\">\r\n              <div class=\"row g-3\">\r\n                <div class=\"col-12\"><label for=\"username\" class=\"form-label\">이름</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"username\"\r\n                         placeholder=\"Username\"\r\n                         v-model=\"state.form.name\">\r\n                </div>\r\n                <div class=\"col-12\"><label for=\"address\" class=\"form-label\">전화번호</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"address\"\r\n                         placeholder=\"Phone Number\"\r\n                         v-model=\"state.form.address\">\r\n                </div>\r\n                <div class=\"col-12\"><label for=\"address\" class=\"form-label\">주소</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"address\"\r\n                         placeholder=\"Address\"\r\n                         v-model=\"state.form.address\">\r\n                </div>\r\n                <div class=\"col-12\"><label for=\"address\" class=\"form-label\">배송 요청사항</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"address\"\r\n                         placeholder=\"Request\"\r\n                         v-model=\"state.form.address\">\r\n                </div>\r\n              </div>\r\n              <hr class=\"my-4\">\r\n              <h4 class=\"mb-3\">결제 수단</h4>\r\n              <div class=\"my-3\">\r\n                <div class=\"form-check\">\r\n                  <input id=\"credit\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\" checked v-model=\"state.form.payment\" value=\"card\">\r\n                    <label class=\"form-check-label\" for=\"card\">신용카드</label>\r\n                </div>\r\n                <div class=\"form-check\">\r\n                  <input id=\"bank\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\" v-model=\"state.form.payment\" value=\"bank\">\r\n                  <label class=\"form-check-label\" for=\"bank\">무통장입금</label>\r\n                </div>\r\n              </div>\r\n              <label for=\"cc-number\" class=\"form-label\">카드 번호</label>\r\n              <input\r\n                  type=\"text\" class=\"form-control\" id=\"cc-number\" v-model=\"state.form.cardNumber\">\r\n              <hr class=\"my-4\">\r\n              <button class=\"w-100 btn btn-primary btn-lg\" @click=\"submit()\">결제하기</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n</style>"],"mappings":";AACA,SAAQA,QAAQ,EAAEC,QAAQ,QAAO,KAAK;AACtC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAAQC,SAAS,QAAO,eAAe;AACvC,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;MACrBM,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;QACJC,IAAI,EAAC,EAAE;QACPC,OAAO,EAAC,EAAE;QACVC,OAAO,EAAC,EAAE;QACVC,UAAU,EAAC,EAAE;QACbL,KAAK,EAAC;MAER;IACF,CAAC;IACD,MAAMM,IAAG,GAAIA,CAAA,KAAM;MACjBX,KAAK,CAACY,GAAG,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC;QAACC;MAAI,CAAC,KAAK;QAC5CC,OAAO,CAACC,GAAG,CAACF,IAAI,CAAC;QACjBV,KAAK,CAACC,KAAI,GAAIS,IAAI;MACpB,CAAC;IACH;IACA,MAAMG,MAAK,GAAIA,CAAA,KAAM;MACnB,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAACjB,KAAK,CAACE,IAAI,CAAC,CAAC;MACnDY,IAAI,CAACb,KAAI,GAAIc,IAAI,CAACE,SAAS,CAACjB,KAAK,CAACC,KAAK,CAAC;MACxCL,KAAK,CAACsB,I